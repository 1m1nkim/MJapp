<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/some_id"
    android:layout_width="855px"
    android:layout_height="566px"
    android:clipToOutline="true"
    android:background="@drawable/member"
    >

    <RelativeLayout
        android:id="@+id/re_price_dio"
        android:layout_width="855px"
        android:layout_height="566px"
        android:background="@drawable/membership"
        android:clipToOutline="true">

        <TextView
            android:id="@+id/text_price"
            android:layout_width="200px"
            android:layout_height="66px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="50px"
            android:layout_marginTop="61px"
            android:gravity="center_vertical"
            android:text="설정 수정"
            android:textAppearance="@style/some_id" />
        <View
            android:id="@+id/rectangle_7"
            android:layout_width="700px"
            android:layout_height="225px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="50px"
            android:layout_marginTop="168px"
            android:background="@drawable/explain_re_d"
            />

        <EditText
            android:id="@+id/ed_explain"
            android:layout_width="700px"
            android:layout_height="225px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="55px"
            android:layout_marginTop="168px"
            android:background="@null"
            android:gravity="center_vertical"
            android:hint="설명을 작성해주세요." />


        <TextView
            android:id="@+id/submit_btn_rep_txt"
            android:layout_width="200px"
            android:layout_height="66px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="700px"
            android:layout_marginTop="450px"
            android:gravity="center_vertical"
            android:text="저장"
            android:textAppearance="@style/some_id" />

        <TextView
            android:id="@+id/cancle_btn_rep_txt"
            android:layout_width="200px"
            android:layout_height="66px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="550px"
            android:layout_marginTop="450px"
            android:gravity="center_vertical"
            android:text="취소"
            android:textAppearance="@style/some_id" />

        <android.widget.Button
            android:id="@+id/submit_btn_reex"
            android:layout_width="100px"
            android:layout_height="66px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="700px"
            android:layout_marginTop="450px"
            android:background="#00ff0000"
            android:gravity="center_vertical" />

        <android.widget.Button
            android:id="@+id/cancle_btn_reex"
            android:layout_width="100px"
            android:layout_height="66px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="550px"
            android:layout_marginTop="450px"
            android:background="#00ff0000"
            android:gravity="center_vertical" />

    </RelativeLayout>
</RelativeLayout>