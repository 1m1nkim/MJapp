<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/some_id"
    android:layout_width="855px"
    android:layout_height="376px"
    android:clipToOutline="true"
    android:background="@drawable/member"
    >

    <RelativeLayout
        android:id="@+id/re_price_dio"
        android:layout_width="855px"
        android:layout_height="376px"
        android:background="@drawable/membership"
        android:clipToOutline="true">

        <TextView
            android:id="@+id/text_price"
            android:layout_width="200px"
            android:layout_height="66px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="50px"
            android:layout_marginTop="61px"
            android:gravity="center_vertical"
            android:text="가격 수정"
            android:textAppearance="@style/some_id" />

        <EditText
            android:id="@+id/ed_price"
            android:layout_width="700px"
            android:layout_height="66px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="50px"
            android:layout_marginTop="168px"
            android:background="@null"
            android:gravity="center_vertical"
            android:hint="5000" />

        <View
            android:id="@+id/line_69"
            android:layout_width="734px"
            android:layout_height="1px"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="240px"
            android:background="@drawable/line_69" />

        <TextView
            android:id="@+id/submit_btn_rep_txt"
            android:layout_width="200px"
            android:layout_height="66px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="700px"
            android:layout_marginTop="280px"
            android:gravity="center_vertical"
            android:text="저장"
            android:textAppearance="@style/some_id" />
        <TextView
            android:id="@+id/cancle_btn_rep_txt"
            android:layout_width="200px"
            android:layout_height="66px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="550px"
            android:layout_marginTop="280px"
            android:gravity="center_vertical"
            android:text="취소"
            android:textAppearance="@style/some_id" />
        <android.widget.Button
            android:id="@+id/submit_btn_rep"
            android:layout_width="100px"
            android:layout_height="66px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="700px"
            android:layout_marginTop="280px"
            android:gravity="center_vertical"
            android:background="#00ff0000"/>
        <android.widget.Button
            android:id="@+id/cancle_btn_rep"
            android:layout_width="100px"
            android:layout_height="66px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="550px"
            android:layout_marginTop="280px"
            android:gravity="center_vertical"
            android:background="#00ff0000"/>

    </RelativeLayout>
</RelativeLayout>